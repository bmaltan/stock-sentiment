{
    "hosting": {
        "public": "dist/apps/frontend",
        "predeploy": [
            "ng build --prod"
        ],
        "ignore": [
            "firebase.json",
            "**/.*",
            "**/node_modules/**"
        ],
        "rewrites": [
            {
                "source": "**",
                "destination": "/index.html"
            }
        ]
    }
}