{
    "hosting": {
        "public": "dist/apps/frontend",
        "predeploy": [
            "npm run lint frontend",
            "npm run build frontend --prod"
        ],
        "ignore": [
            "firebase.json",
            "**/.*",
            "**/node_modules/**"
        ],
        "rewrites": [
            {
                "source": "**",
                "destination": "/index.html"
            }
        ]
    }
}